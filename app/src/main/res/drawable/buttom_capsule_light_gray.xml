        android:bottom="0px"
        android:left="0px"
        android:right="0px"
        android:top="0px">
        <shape android:shape="rectangle">
            <solid android:color="@color/automation_gray" />
            <corners
                android:bottomLeftRadius="10dp"
                android:bottomRightRadius="10dp"
                android:topLeftRadius="10dp"
                android:topRightRadius="10dp" />

        </shape>
    </item>
</layer-list>


